{
  "name": "dgg-chat-gui",
  "version": "2.2.13",
  "description": "Destiny.gg chat client front-end",
  "main": "destiny",
  "scripts": {
    "tld": "node scripts/tld.js",
    "glue": "node scripts/glue.js",
    "start": "webpack-dev-server --verbose --debug --mode=development",
    "static": "cp -r assets/emotes/emoticons static/img/ && cp -r assets/emotes/emoticons-animated static/img/ && cp dist/chat.js static/chat.js && cp -r assets/emotes/ static/emotes/",
    "build": "npm run tld && webpack && npm run static"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/destinygg/chat-gui.git"
  },
  "keywords": [
    "destiny.gg",
    "destinygg",
    "dgg"
  ],
  "author": "Destiny.gg Foundation",
  "contributors": [],
  "homepage": "http://www.destiny.gg/",
  "license": "SEE LICENSE IN <LICENSE.md>",
  "bugs": "https://github.com/destinygg/website/issues",
  "directories": {
    "assets": "./assets"
  },
  "dependencies": {
    "jquery": "~3.3.1",
    "moment": "~2.22.2",
    "nanoscroller": "~0.8.7",
    "normalize.css": "~8.0.1",
    "throttle-debounce": "~2.0.1",
    "webpack-manifest-plugin": "^2.0.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "~9.3.1",
    "babel-core": "~7.0.0-0",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "~6.24.1",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^1.0.1",
    "dotenv": "^6.2.0",
    "file-loader": "~2.0.0",
    "html-loader": "^0.5.5",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "~4.10.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "spritesmith": "~3.3.1",
    "style-loader": "~0.23.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "~3.1.10"
  },
  "engines": {
    "node": "~5.0.2",
    "npm": "~3.3.12"
  }
}
