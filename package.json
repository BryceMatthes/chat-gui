{
  "name": "dgg-chat-gui",
  "version": "2.1.24",
  "description": "Destiny.gg chat client front-end",
  "main": "destiny",
  "scripts": {
    "tld": "node scripts/tld.js",
    "glue": "node scripts/glue.js",
    "static": "cp assets/index.html static/index.html && cp assets/chatstreamed.html static/chatstreamed.html && cp -r assets/emotes/emoticons static/img/ && cp -r assets/emotes/emoticons-animated static/img/",
    "build": "npm run tld && webpack && npm run static",
    "build:production": "npm run tld && webpack -p && npm run static",
    "watch": "webpack -w",
    "start": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/destinygg/chat-gui.git"
  },
  "keywords": [
    "destiny.gg",
    "destinygg",
    "dgg"
  ],
  "author": "Destiny.gg Foundation",
  "contributors": [],
  "homepage": "http://www.destiny.gg/",
  "license": "SEE LICENSE IN <LICENSE.md>",
  "bugs": "https://github.com/destinygg/website/issues",
  "directories": {
    "assets": "./assets",
    "static": "./static"
  },
  "devDependencies": {
    "autoprefixer": "~8.0.0",
    "babel-core": "~6.26.0",
    "babel-loader": "~7.1.2",
    "babel-preset-es2015": "~6.24.1",
    "clean-webpack-plugin": "~0.1.18",
    "core-js": "~2.5.3",
    "css-loader": "~0.28.9",
    "dotenv": "^5.0.1",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "^1.1.7",
    "font-awesome": "~4.7.0",
    "jquery": "~3.3.1",
    "jquery-validation": "~1.17.0",
    "json-loader": "~0.5.7",
    "moment": "~2.20.1",
    "nanoscroller": "~0.8.7",
    "node-sass": "~4.7.2",
    "normalize.css": "~8.0.0",
    "postcss-loader": "~2.1.0",
    "sass-loader": "~6.0.6",
    "spritesmith": "~3.3.0",
    "style-loader": "~0.20.2",
    "throttle-debounce": "~1.0.1",
    "timestring": "^5.0.0",
    "ts-loader": "~3.5.0",
    "typescript": "~2.7.2",
    "webpack": "~3.11.0",
    "webpack-dev-server": "~2.11.1"
  },
  "engines": {
    "node": "~5.0.2",
    "npm": "~3.3.12"
  }
}
